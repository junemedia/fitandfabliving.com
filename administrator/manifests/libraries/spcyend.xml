<?xml version="1.0" encoding="utf-8"?>
<extension type="library" version="2.5" method="upgrade" >
    <name>SP CYEND</name>
    <libraryname>spcyend</libraryname>
    <creationDate>August 2013</creationDate>
    <author>SP CYEND</author>
    <authorEmail>info@cyend.com</authorEmail>
    <authorUrl>http://www.cyend.com</authorUrl>
    <copyright>SP CYEND - All rights reserved.</copyright>
    <license>GNU/GPLv3 http://www.gnu.org/licenses/gpl-3.0.html</license>
    <!--  The version string is recorded in the components table -->
    <version>3.1.3</version>   
    <description>SP CYEND Libraries</description>

    <files folder="libraries">
        <filename>index.html</filename>
        <folder>utilities</folder>
        <folder>database</folder>
    </files>
    
    <media destination="spcyend" folder="media">
        <filename>index.html</filename>
        <folder>css</folder>
        <folder>images</folder>
        <folder>js</folder>
        <folder>swf</folder>
        <folder>various</folder>
    </media>
    
    <languages folder="language">
        <language tag="en-GB">en-GB.lib_spcyend.ini</language>
    </languages>
            
</extension>
